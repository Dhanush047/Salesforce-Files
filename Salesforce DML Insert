SALESFORCE DML FOR INSERT
=====================================================

public class accrec 
{
    public static void record()
    {
        account acc = new account();
        acc.Name = 'Dhoni test';
        acc.Phone = '8523697415';
        insert acc;
        
    }
    
    public static void rec()
    {
        account ac = new account(name = 'Sachin',phone = '8855414778', AnnualRevenue = 80000);
        insert ac;        
        
    }
